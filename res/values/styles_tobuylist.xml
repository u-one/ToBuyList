<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AutoCompleteTextViewToBuyList" parent="android:Widget.Holo.Light.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/tobuylist_list_selector_holo_light</item>
        <item name="android:background">@drawable/tobuylist_edit_text_holo_light</item>
    </style>

    <style name="ButtonToBuyList" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/tobuylist_btn_default_holo_light</item>
    </style>

    <style name="ImageButtonToBuyList" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/tobuylist_btn_default_holo_light</item>
    </style>

    <style name="SpinnerToBuyList" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/tobuylist_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/tobuylist_list_selector_holo_light</item>
    </style>

    <style name="TabToBuyList">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/tobuylist_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextToBuyList">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarToBuyList" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/tobuylist_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/tobuylist_progress_indeterminate_horizontal_holo_light</item>
    </style>

    <style name="ToggleToBuyList" parent="android:Widget.Holo.Light.Button.Toggle">
        <item name="android:background">@drawable/tobuylist_btn_toggle_holo_light</item>
    </style>
    
    <!-- ActionBar -->

    <style name="ActionBar.Solid.Tobuylist" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_solid_tobuylist</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_tobuylist</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_tobuylist</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Tobuylist</item>
    </style>

    <style name="ActionBar.Transparent.Tobuylist" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_tobuylist</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Tobuylist</item>
    </style>

    <style name="PopupMenu.Tobuylist" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_tobuylist</item>
    </style>

    <style name="DropDownListView.Tobuylist" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_tobuylist</item>
    </style>

    <style name="ActionBarTabStyle.Tobuylist" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_tobuylist</item>
    </style>

    <style name="DropDownNav.Tobuylist" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_tobuylist</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_tobuylist</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_tobuylist</item>
    </style>

    <style name="ProgressBar.Tobuylist" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_tobuylist</item>
    </style>

    <style name="ActionButton.CloseMode.Tobuylist" parent="@android:style/Widget.Holo.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_tobuylist</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Tobuylist.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Tobuylist</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Tobuylist</item>
    </style>

</resources>